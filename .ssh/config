Host *
    UseKeyChain yes
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa

# Personal
Host dev-atm-dev-sea-vm
    HostName 10.2.0.4
    User van
	ForwardAgent yes
    LocalForward 5000 localhost:5000

# Work
Host ppg-exp-pro-sea-vmw
    HostName 10.4.0.4
    Port 2222
    User van
	ForwardAgent yes
    ServerAliveInterval 60
    LocalForward 7071 localhost:7071
